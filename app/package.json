{
    "name": "ew",
    "version": "1.0.0",
    "description": "Ew Box",
    "main": "./main-dist/index.js",
    "scripts": {
        "lint": "eslint ./render",
        "lint:fix": "eslint ./render --fix",
        "pri": "prisma generate",
        "start:main": "tsc -w --p tsconfig.main.json",
        "start:react": "bash start.sh && cross-env PORT=31116 vite --config vite.config.ts",
        "build:react": "vite build",
        "build:main": "tsc --p tsconfig.main.json",
        "start:electron": "electron ./main-dist/index",
        "build:mac": "electron-builder --mac",
        "build:win": "electron-builder --win",
        "postinstall": "electron-builder install-app-deps && electron-rebuild"
    },
    "build": {
        "extraResources": [
            {
                "from": "node_modules/@prisma/engines/libquery_engine-darwin-arm64.dylib.node",
                "to": "resources/engines/libquery_engine-darwin-arm64.dylib.node"
            },
            {
                "from": "../prisma/migrations",
                "to": "resources/prisma/migrations"
            },
            {
                "from": "../prisma/schema.prisma",
                "to": "resources/prisma/schema.prisma"
            }
        ],
        "files": [
            "package.json",
            "prisma/migrations/**/*",
            "main-dist/**/*",
            "render-dist/**/*"
        ],
        "asar": true,
        "asarUnpack": [
            "**/node_modules/prisma/**/*",
            "**/node_modules/@prisma/**/*",
            "**/node_modules/sharp/**/*",
            "**/node_modules/@img/**/*"
        ],
        "productName": "Ew Box",
        "appId": "org.xiaoming.developer",
        "mac": {
            "icon": "assets/ew.icns",
            "type": "distribution",
            "target": [
                "dmg",
                "zip"
            ]
        },
        "win": {
            "icon": "build/favicon.ico",
            "target": [
                "nsis"
            ]
        },
        "nsis": {
            "oneClick": false,
            "perMachine": true,
            "allowToChangeInstallationDirectory": true,
            "createDesktopShortcut": true,
            "createStartMenuShortcut": false
        }
    },
    "engines": {
        "node": ">=21.1.0"
    },
    "keywords": [],
    "author": "xiaoming",
    "license": "ISC",
    "devDependencies": {
        "@electron/rebuild": "^3.6.0",
        "@types/koa-bodyparser": "^4.3.12",
        "@types/koa-router": "^7.4.8",
        "@types/koa__cors": "^5.0.0",
        "@types/lodash-es": "^4.17.12",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^8.7.0",
        "@typescript-eslint/parser": "^8.7.0",
        "@vitejs/plugin-react": "^4.3.1",
        "autoprefixer": "^10.4.19",
        "chokidar": "^3.6.0",
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.3.0",
        "electron": "32.1.2",
        "electron-builder": "^24.13.3",
        "electron-reloader": "^1.2.1",
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-jsx-a11y": "^6.10.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.36.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "fast-glob": "^3.3.2",
        "file-loader": "^6.2.0",
        "globals": "^15.9.0",
        "html-webpack-plugin": "^5.3.2",
        "less": "^4.2.0",
        "less-loader": "^10.0.1",
        "postcss": "^8.4.39",
        "prebuild-install": "^7.1.2",
        "prettier": "^3.2.5",
        "style-loader": "^3.3.0",
        "tailwindcss": "^3.4.4",
        "ts-loader": "^9.2.6",
        "typescript": "^5.4.5",
        "typescript-eslint": "^8.7.0",
        "url-loader": "^4.1.1",
        "vite": "^5.2.11",
        "vite-plugin-svg-icons": "^2.0.1"
    },
    "dependencies": {
        "@koa/cors": "^5.0.0",
        "@prisma/client": "5.18.0",
        "clsx": "^2.1.1",
        "crypto": "^1.0.1",
        "dayjs": "^1.11.11",
        "decimal.js": "^10.4.3",
        "electron-is-dev": "^2.0.0",
        "esbuild": "^0.21.3",
        "exceljs": "^4.4.0",
        "image-size": "^1.1.1",
        "koa": "^2.15.3",
        "koa-bodyparser": "^4.4.1",
        "koa-router": "^12.0.1",
        "lodash": "^4.17.21",
        "log4js": "^6.9.1",
        "node-machine-id": "^1.1.12",
        "prisma": "^5.18.0",
        "qs": "^6.13.0",
        "sharp": "0.32.6",
        "uuid": "^9.0.1"
    }
}