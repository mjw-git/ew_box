{
  "name": "ew",
  "version": "1.0.0",
  "description": "Ew Box",
  "main": "./main-dist/index.js",
  "scripts": {
    "pri": "prisma generate",
    "start:main": "tsc -w --p tsconfig.main.json",
    "start:react": "bash start.sh && vite --config vite.config.ts",
    "build:react": "vite build",
    "build:main": "tsc --p tsconfig.main.json",
    "start:electron": "electron ./main-dist/index",
    "build:mac": "node scripts/build.js && vite build && electron-builder --mac",
    "build:win": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps && electron-rebuild"
  },
  "build": {
    "extraResources": [
      {
        "from": "node_modules/@prisma/engines/libquery_engine-darwin-arm64.dylib.node",
        "to": "resources/engines/libquery_engine-darwin-arm64.dylib.node"
      },
      {
        "from": "prisma/migrations",
        "to": "resources/prisma/migrations"
      },
      {
        "from": "prisma/schema.prisma",
        "to": "resources/prisma/schema.prisma"
      }
    ],
    "files": [
      "package.json",
      "prisma/migrations/**/*",
      "main-dist/**/*",
      "node_modules/**/*",
      "render/dist/**/*"
    ],
    "asar": true,
    "asarUnpack": [
      "**/node_modules/prisma/**/*",
      "**/node_modules/@prisma/**/*",
      "**/node_modules/sharp/**/*",
      "**/node_modules/@img/**/*"
    ],
    "productName": "Ew Box",
    "appId": "org.xiaoming.developer",
    "mac": {
      "icon": "assets/ew.icns",
      "type": "distribution",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "build/favicon.ico",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": false
    }
  },
  "keywords": [],
  "author": "xiaoming",
  "license": "ISC",
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/koa__cors": "^5.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "chokidar": "^3.6.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.3.0",
    "electron": "^15.5.7",
    "electron-builder": "^24.13.3",
    "electron-reloader": "^1.2.1",
    "fast-glob": "^3.3.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "less": "^4.2.0",
    "less-loader": "^10.0.1",
    "postcss": "^8.4.39",
    "prebuild-install": "^7.1.2",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.0",
    "tailwindcss": "^3.4.4",
    "ts-loader": "^9.2.6",
    "typescript": "^5.4.5",
    "url-loader": "^4.1.1",
    "vite": "^5.2.11",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@koa/cors": "^5.0.0",
    "@prisma/client": "5.18.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@react-spring/web": "^9.7.3",
    "@types/qs": "^6.9.15",
    "@types/uuid": "^9.0.8",
    "@use-gesture/react": "^10.3.1",
    "ahooks": "^3.8.0",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.11",
    "electron-is-dev": "^2.0.0",
    "esbuild": "^0.21.3",
    "image-size": "^1.1.1",
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^12.0.1",
    "localforage": "^1.10.0",
    "log4js": "^6.9.1",
    "lucide-react": "^0.407.0",
    "node-machine-id": "^1.1.12",
    "prisma": "^5.18.0",
    "qs": "^6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-router-dom": "^6.23.1",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  }
}
